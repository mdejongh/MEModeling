	"visible": true,
	"categories": ["active"],
	"widgets": {
		"input": null,
		"output": "kbaseReportView"
	},
	"parameters": [ 
		{
			"id": "model_ref",
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "" ],
			"field_type": "text",
			"text_options": {
				"valid_ws_types": ["KBaseFBA.FBAModel"]
			}
		}
	],
	"behavior": {
		"service-mapping": {
			"url": "",
			"name": "MEModeling",
			"method": "build_me_model",
			"input_mapping": [
        {
          "input_parameter": "model_ref",
          "target_property": "model_ref",
	  "target_type_transform": "ref"
